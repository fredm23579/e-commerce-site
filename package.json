{
  "name": "e-commerce-site",
  "version": "1.0.0",
  "description": "A MERN E-Commerce site with payments and login/sign-up portal",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "watch": "nodemon",
    "seed": "node server/config/seeds.js",
    "render-build": "npm install && npm run seed && npm install --prefix client && npm run build --prefix client"
  },
  "keywords": ["MongoDB", "React", "GraphQL", "JWT", "Stripe", "Express", "bcrypt", "Mongoose"],
  "author": "Fred Motta",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.7.2",
    "apollo-server-express": "^3.13.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.1",
    "nodemon": "^2.0.2",
    "concurrently": "^7.2.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
