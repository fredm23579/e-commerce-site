{
  "name": "e-commerce-site",
  "private": true,
  "version": "1.0.0",
  "description": "A MERN E-Commerce site with payments and login/sign-up portal with Stripe integration",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js & node client/build.js",
    "build": "nodemon --watch server --exec node server/server.js & nodemon --watch client --exec node client/build.js & npm run render-build & npm run seed & npm run watch",
    "test": "jest",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prettier": "prettier . --write",
    "prettier-check": "prettier . --check",
    "watch": "nodemon --watch server --exec node server/server.js & nodemon --watch client --exec node client/build.js & npm run render-build & npm run seed & npm run watch",
    "seed": "node server/config/seeds.js",
    "render-build": "npm install && npm run seed && npm install --prefix client --legacy-peer-deps && npm run build --prefix client --legacy-peer-deps"
  },
  "keywords": ["MongoDB", "React", "GraphQL", "JWT", "Stripe", "Express", "bcrypt", "Mongoose"],
  "author": "Fred Motta",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@apollo/react-hooks": "^4.0.0",
    "@stripe/react-stripe-js": "^2.1.0",
    "@stripe/stripe-js": "^1.38.0",
    "@testing-library/react": "^12.1.2",
    "axios": "^1.3.4",
    "@apollo/server": "^4.7.2",
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^2.4.3",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cors-anywhere": "^0.4.4",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "dotenv": "^16.0.3",
    "express-graphql": "^0.12.0",
    "graphql-scalars": "^2.0.0",
    "express": "^4.19.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "graphql": "^16.8.1",
    "graphql-request": "^5.2.0",
    "graphql-server-express": "^2.24.0",
    "graphql-tag": "^2.12.6",
    "html-react-parser": "^3.0.4",
    "isomorphic-fetch": "^3.0.0",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "mongoose-unique-validator": "^1.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^4.0.2",
    "mongoose": "^8.4.1",
    "prop-types": "^15.8.1",
    "query-string": "^8.1.0",
    "querystring": "^0.2.1",
    "react-apollo": "^3.1.5",
    "stripe": "^11.15.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "vite-tsconfig-paths": "^4.0.8",
    "vite": "^4.5.3",
    "vitest": "^0.25.8",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^2.4.0",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.3.3",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "vite-tsconfig-paths": "^4.0.8",
    "vitest": "^0.25.8",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^2.4.0",
    "concurrently": "^7.0.0"
  }
}
