{
  "name": "e-commerce-site",
  "private": true,
  "version": "1.0.0",
  "description": "A MERN E-Commerce site with payments and login/sign-up portal with Stripe integration",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js & node client/build.js",
    "build": "nodemon --watch server --exec node server/server.js & nodemon --watch client --exec node client/build.js & npm run render-build & npm run seed & npm run watch",
    "test": "jest",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prettier": "prettier . --write",
    "prettier-check": "prettier . --check",
    "watch": "nodemon --watch server --exec node server/server.js & nodemon --watch client --exec node client/build.js & npm run render-build & npm run seed & npm run watch",
    "seed": "node server/config/seeds.js",
    "render-build": "npm install && npm run seed && npm install --prefix client --legacy-peer-deps && npm run build --prefix client --legacy-peer-deps"
  },
  "keywords": ["MongoDB", "React", "GraphQL", "JWT", "Stripe", "Express", "bcrypt", "Mongoose"],
  "author": "Fred Motta",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "latest",
    "@apollo/react-hooks": "latest",
    "@stripe/react-stripe-js": "latest",
    "@stripe/stripe-js": "latest",
    "@testing-library/react": "latest",
    "axios": "latest",
    "@apollo/server": "latest",
    "apollo-server-express":"latest",
    "bcryptjs": "latest",
    "bcrypt": "latest",
    "cors": "latest",
    "cors-anywhere": "latest",
    "date-fns": "latest",
    "date-fns-tz": "latest",
    "dotenv": "latest",
    "express-graphql": "latest",
    "graphql-scalars": "latest",
    "express": "latest",
    "express-session": "latest",
    "express-validator":"latest",
    "graphql": "latest",
    "graphql-request": "latest",
    "graphql-server-express": "latest",
    "graphql-tag": "latest",
    "html-react-parser":"latest",
    "isomorphic-fetch": "latest",
    "jsdom": "latest",
    "lodash": "latest",
    "mongoose-unique-validator": "latest",
    "moment": "latest",
    "moment-timezone": "latest",
    "jsonwebtoken": "latest",
    "nanoid": "latest",
    "mongoose": "latest",
    "prop-types": "latest",
    "query-string": "latest",
    "querystring": "latest",
    "react-apollo": "latest",
    "stripe": "latest",
    "ts-node": "latest",
    "tsconfig-paths": "latest",
    "typescript": "latest",
    "vite-tsconfig-paths": "latest",
    "vite": "latest",
    "vitest": "latest",
    "vite-plugin-eslint": "latest",
    "vite-plugin-svgr": "latest",
    "nodemon": "latest"
  },
  "devDependencies": {
    "nodemon": "latest",
    "prettier": "latest",
    "pretty-quick": "latest",
    "supertest": "latest",
    "jest": "latest",
    "ts-jest": "latest",
    "ts-node": "latest",
    "tsconfig-paths": "latest",
    "typescript": "latest",
    "vite-tsconfig-paths": "latest",
    "vitest": "latest",
    "vite-plugin-eslint": "latest",
    "vite-plugin-svgr": "latest",
    "concurrently": "latest"
  }
}
